<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.cqupt.mislab.excellentroom.dao.ProjectDao">
    <resultMap id="Project" type="cn.edu.cqupt.mislab.excellentroom.domain.entity.Project">
        <result property="projectId" column="projectId" jdbcType="VARCHAR"/>
    </resultMap>

    <select id="selectProjectById" resultMap="Project" resultType="cn.edu.cqupt.mislab.excellentroom.domain.entity.Project">
        select projectId
        from t_project_information
        where projectId = #{projectId}
    </select>


</mapper>